          ID.29 = IDBytes(4).5
          ID.30 = IDBytes(4).6
          ID.31 = IDBytes(4).7

          CanBus_SetFilter 0,1,ID

        end if
      end if

    case 112
      if datan = 0 then
        datal=USBTempBuffer(2)
        datan = 1
      else
        IDbytes(datan)=USBTempBuffer(2)
        datan += 1
        if datan = datal +1 then
          datan=0

          ID = 0
          ID.0 = IDBytes(1).0
          ID.1 = IDBytes(1).1
          ID.2 = IDBytes(1).2
          ID.3 = IDBytes(1).3
          ID.4 = IDBytes(1).4
          ID.5 = IDBytes(1).5
          ID.6 = IDBytes(1).6
          ID.7 = IDBytes(1).7

          ID.8 = IDBytes(2).0
