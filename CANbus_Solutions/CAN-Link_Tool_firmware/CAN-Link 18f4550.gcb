          ID.29 = IDBytes(4).5
          ID.30 = IDBytes(4).6
          ID.31 = IDBytes(4).7

          CanBus_SetFilter 0,0,ID

        end if
      end if

    case 111
      if datan = 0 then
          datal=USBTempBuffer(2)
          datan = 1
        else
          IDbytes(datan)=USBTempBuffer(2)
        datan += 1
        if datan = datal +1 then
          datan=0

          ID = 0
          ID.0 = IDBytes(1).0
          ID.1 = IDBytes(1).1
          ID.2 = IDBytes(1).2
          ID.3 = IDBytes(1).3
          ID.4 = IDBytes(1).4
          ID.5 = IDBytes(1).5
          ID.6 = IDBytes(1).6
          ID.7 = IDBytes(1).7

          ID.8 = IDBytes(2).0
          ID.9 = IDBytes(2).1
          ID.10 = IDBytes(2).2
          ID.11 = IDBytes(2).3
          ID.12 = IDBytes(2).4
          ID.13 = IDBytes(2).5
          ID.14 = IDBytes(2).6
          ID.15 = IDBytes(2).7

          ID.16 = IDBytes(3).0
          ID.17 = IDBytes(3).1
          ID.18 = IDBytes(3).2
          ID.19 = IDBytes(3).3
          ID.20 = IDBytes(3).4
          ID.21 = IDBytes(3).5
          ID.22 = IDBytes(3).6
